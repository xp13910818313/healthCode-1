page {
  width: 100%;
  height: 100%;
  /* background: url('https://timgsa.baidu.com/timg?image&quality=80&size=b9999_10000&sec=1591079191735&di=5d13bd788706b35e3ea0183351526ffb&imgtype=0&src=http%3A%2F%2Fpic1.win4000.com%2Fwallpaper%2F2017-12-09%2F5a2baaf25b957.jpg') no-repeat center; */
}

.clearfix::after {
  content: '';
  display: block;
  clear: both;
}

.fl {
  float: left;
}

.fr {
  float: right;
}

.title {
  font-size: 50rpx;
  font-weight: 700;
  color: #000;
}

.index {
  box-sizing: border-box;
  padding: 60rpx;
  width: 100%;
  height: 100%;
  position: relative;
  /* background: #999; */
}

.bgImg {
  margin-top: 110rpx;
}

.list-box {
  width: 100%;
  margin-top: 120rpx;
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
}

.list {
  width: calc((100% - 52rpx) / 3);
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
  border: 1px solid #999;
  margin: 0 20rpx 20rpx 0;
  float: left;
  border-radius: 10rpx;
  box-shadow: 0 0 3px #999;
}

.list:nth-child(3n) {
  margin-right: 0;
}

.btn {
  position: absolute;
  bottom: 100rpx;
  width: calc(100% - 120rpx);
}
.btn-box{
  background:linear-gradient(70deg,#11998e,#1dc05b);
  color: #fff;
}
.mine{
  text-align: center;
  color: #6b6b6b;
  font-size: 28rpx;
  text-decoration: underline;
  padding: 40rpx 0;
}

.banben{
  text-align: center;
  font-size: 20rpx;
  box-sizing: border-box;
  padding: 0 30px;
  color: #999;
}