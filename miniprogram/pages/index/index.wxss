page {
  width: 100%;
  height: 100%;
  background: url('http://20200308323095.yunhaiweb.com/upload/document5/57257/image/20200602140229794.jpg') no-repeat  center;
  background-size: 100% 110%;
  background-attachment:fixed;
}

.clearfix::after {
  content: '';
  display: block;
  clear: both;
}

.fl {
  float: left;
}

.fr {
  float: right;
}

.title {
  font-size: 50rpx;
  font-weight: 700;
  color: #000;
}

.index {
  box-sizing: border-box;
  padding: 60rpx;
  width: 100%;  
  height: 100%;
  position: relative;
  /* background: #999; */
}

.bgImg {
  margin: 130rpx auto ;
  width: 100%;
}
.bgImg .img {
  width: 100%;
}

.list-box {
  width: 100%;
  margin-top: 120rpx;
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
}

.list {
  width: calc((100% - 52rpx) / 3);
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
  border: 1px solid #999;
  margin: 0 20rpx 20rpx 0;
  float: left;
  border-radius: 10rpx;
  box-shadow: 0 0 3px #999;
}

.list:nth-child(3n) {
  margin-right: 0;
}

.btn {
  position: absolute;
  bottom: 100rpx;
  width: calc(100% - 120rpx);
}
.btn-box{
  background:linear-gradient(70deg,#11998e,#1dc05b);
  color: #fff;
}
.mine{
  text-align: center;
  color: #6b6b6b;
  font-size: 28rpx;
  text-decoration: underline;
  padding: 40rpx 0;
}

.banben{
  text-align: center;
  font-size: 20rpx;
  box-sizing: border-box;
  padding: 0 30px;
  color: #999;
}